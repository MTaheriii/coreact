{
  "name": "coreact",
  "version": "1.0.0",
  "description": "react core architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --pretty",
    "ship": "npm run build && npm publish",
    "build:example": "rm -rf ./example-dist ./example-bundle && tsc --pretty --project tsconfig-example.json",
    "pack:dev": "NODE_ENV=development webpack --config webpack.config.js --mode=development --profile --colors",
    "pack:production": "NODE_ENV=production webpack --config webpack.config.js --mode=production --profile --colors",
    "clean": "rm -rf ./example-dist ./example-bundle ./dist",
    "watch": "npm run build:example && NODE_ENV=development node ./example-dist/example/server.js",
    "prepare": "npm run build:example && npm run pack:production",
    "start": "NODE_ENV=production node ./example-dist/example/server.js"
  },
  "homepage": "https://github.com/xeuus/coreact",
  "author": "Aryan Alikhani",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xeuus/coreact.git"
  },
  "bugs": {
    "url": "https://github.com/xeuus/coreact/issues"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.17.1",
    "@types/node": "^11.13.18",
    "@types/pako": "^1.0.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/webpack": "^4.39.1",
    "@types/webpack-env": "^1.14.0",
    "autoprefixer": "^9.6.1",
    "compression-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "pako": "^1.0.10"
  }
}
